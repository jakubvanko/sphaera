{
  "extends": [
    "react-app",
    "airbnb",
    "airbnb/hooks",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y", "react", "prettier"],
  "rules": {
    "react/jsx-curly-brace-presence": [
      2,
      {
        "props": "always",
        "children": "never"
      }
    ]
  }
}
